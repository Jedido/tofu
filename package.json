{
  "name": "tofu",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix",
    "dev": "nodemon start --ext vue",
    "start": "npm run build && npx ts-node server/server.js",
    "prod": "npm run build && npx ts-node server/server.js prod"
  },
  "dependencies": {
    "@vue/component-compiler-utils": "^3.3.0",
    "body-parser": "^1.20.3",
    "bootstrap-icons": "^1.11.3",
    "core-js": "^3.39.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.13",
    "vue-loader": "^17.4.2",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.5.13",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "socket.io": "^4.8.1",
    "tailwindcss": "^3.4.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "vue-axios": "^3.5.2",
    "webpack": "^5.97.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto",
          "semi": false,
          "trailingComma": "es5",
          "indent": 2
        }
      ],
      "prefer-const": [
        "error",
        {
          "destructuring": "any",
          "ignoreReadBeforeAssign": false
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
